"\"$schema\""
"\"$id\""
"\"$ref\""
"\"title\""
"\"description\""
"\"type\""
"\"enum\""
"\"const\""
"\"default\""
"\"examples\""
"\"deprecated\""
"\"readOnly\""
"\"writeOnly\""
"\"format\""
"\"contentEncoding\""
"\"contentMediaType\""
"\"properties\""
"\"patternProperties\""
"\"additionalProperties\""
"\"required\""
"\"minProperties\""
"\"maxProperties\""
"\"items\""
"\"additionalItems\""
"\"prefixItems\""
"\"minItems\""
"\"maxItems\""
"\"uniqueItems\""
"\"contains\""
"\"minimum\""
"\"maximum\""
"\"exclusiveMinimum\""
"\"exclusiveMaximum\""
"\"multipleOf\""
"\"minLength\""
"\"maxLength\""
"\"pattern\""
"\"if\""
"\"then\""
"\"else\""
"\"allOf\""
"\"anyOf\""
"\"oneOf\""
"\"not\""
"\"definitions\""
"\"$defs\""

# --- JSON Schema type values ---
"\"object\""
"\"array\""
"\"string\""
"\"integer\""
"\"number\""
"\"boolean\""
"\"null\""

# --- Reference shapes ---
"\"$ref\":\"#\""
"\"$ref\":\"#/\""
"\"$ref\":\"#/$defs/\""
"\"$ref\":\"#/definitions/\""

# --- Typical schema skeletons ---
"{\"type\":"
"{\"type\":\"object\",\"properties\":{"
"{\"type\":\"array\",\"items\":"
"{\"required\":["
"{\"enum\":["
"{\"oneOf\":["
"{\"anyOf\":["
"{\"allOf\":["
"]}"
"]}"
"]}]}"
"}}"

# --- Formats & patterns ---
"\"format\":\"byte\""
"\"format\":\"hex\""
"\"format\":\"date-time\""
"\"format\":\"uri\""
"\"format\":\"uuid\""
"\"pattern\":\"^0x[0-9a-fA-F]+$\""
"\"pattern\":\"^[A-Za-z0-9+/_=-]{16,}$\""

