      "targets": [
        { "expr": "animica_mempool_held_txs{instance=\"$instance\"}", "legendFormat": "held", "refId": "A" }
      ],
      "title": "Held Queue",
      "type": "stat"
    },

    {
      "datasource": "${DS_PROMETHEUS}",
      "description": "Admission vs rejection rates over time.",
      "fieldConfig": { "defaults": { "unit": "ops", "decimals": 2 }, "overrides": [] },
