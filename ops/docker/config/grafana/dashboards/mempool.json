        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "none" }
      },
      "targets": [
        { "expr": "sum by (instance) (rate(animica_mempool_admit_total{instance=\"$instance\"}[5m]))", "legendFormat": "admit/s", "refId": "A" },
        { "expr": "sum by (instance) (rate(animica_mempool_reject_total{instance=\"$instance\"}[5m]))", "legendFormat": "reject/s", "refId": "B" }
      ],
      "title": "Admissions vs Rejections (rate, 5m)",
      "type": "timeseries"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "description": "Rejections broken down by reason (e.g. low_fee, invalid, ratelimit).",
