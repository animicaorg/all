        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        { "expr": "animica_mempool_ready_txs{instance=\"$instance\"}", "legendFormat": "ready", "refId": "A" }
