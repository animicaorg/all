      "targets": [
        { "expr": "sum by (instance) (rate(animica_mempool_eviction_total{instance=\"$instance\"}[5m]))", "legendFormat": "evictions/s", "refId": "A" },
        { "expr": "sum by (instance) (rate(animica_mempool_replacement_total{instance=\"$instance\"}[5m]))", "legendFormat": "replacements/s", "refId": "B" }
