  "title": "Animica — Mempool (size / admits / rejects / fees / latency)",
  "uid": "animica-mempool-01",
  "version": 1,
  "panels": [
    {
      "datasource": "${DS_PROMETHEUS}",
      "description": "Current total pending transactions in the pool.",
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0 }, "overrides": [] },
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        { "expr": "animica_mempool_pool_size_txs{instance=\"$instance\"}", "legendFormat": "pending txs", "refId": "A" }
      ],
      "title": "Pool Size — Txs",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "description": "Bytes currently held in the mempool.",
      "fieldConfig": { "defaults": { "unit": "bytes", "decimals": 0 }, "overrides": [] },
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 0 },
      "id": 2,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        { "expr": "animica_mempool_pool_size_bytes{instance=\"$instance\"}", "legendFormat": "bytes", "refId": "A" }
      ],
      "title": "Pool Size — Bytes",
      "type": "stat"
    },
    {
      "datasource": "${DS_PROMETHEUS}",
      "description": "Txs ready for inclusion by the block builder.",
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
