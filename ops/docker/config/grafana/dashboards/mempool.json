        "stacking": { "mode": "none" }
      },
      "targets": [
        { "expr": "sum by (reason) (rate(animica_mempool_reject_total{instance=\"$instance\"}[5m]))", "legendFormat": "{{reason}}", "refId": "A" }
      ],
      "title": "Rejects by Reason (rate, 5m)",
      "type": "timeseries"
    },

