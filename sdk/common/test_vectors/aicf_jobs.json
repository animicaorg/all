        "kind": "AI",
        "payload": {
          "model": "text:tiny-1.0",
          "prompt": "hello world",
          "maxTokens": 16
        }
      },
      "expect": {
        "taskId": "0x1111111111111111111111111111111111111111111111111111111111111111",
        "receipt": {
          "taskId": "0x1111111111111111111111111111111111111111111111111111111111111111",
          "enqueuedHeight": 100,
          "kind": "AI",
          "accepted": true
        },
        "resultRecord": {
          "taskId": "0x1111111111111111111111111111111111111111111111111111111111111111",
          "completedHeight": 101,
          "status": "Completed",
          "producer": "anim1prov33r0k3y7q9w8e7r6t5y4u3i2o1p0l9k8j7",
          "costUnits": 42,
          "output": {
            "ai": {
              "text": "hello world âœ¨"
