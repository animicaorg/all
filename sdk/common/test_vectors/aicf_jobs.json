{
  "version": 1,
  "notes": "Deterministic AICF job request/receipt/result vectors used across SDKs. Task IDs are fixed here to provide a single source of truth for round-trip tests. Heights reflect the 'next-block availability' rule (result becomes readable at height+1). Hex fields are 0x-prefixed bytes.",
  "schema": {
    "request.common": [
      "chainId: string (e.g., 'animica:devnet')",
      "height: uint64 (block height at enqueue)",
      "txHash: bytes32_hex",
      "caller: string (bech32m address)",
      "kind: enum['AI','Quantum']",
      "payload: object (see below)"
    ],
    "payload.ai": [
      "model: string (e.g., 'text:tiny-1.0')",
      "prompt: string",
      "maxTokens?: uint32"
    ],
    "payload.quantum": [
      "circuit: object { qubits:uint16, gates:array, measure:array }",
      "shots: uint32",
      "trapsRatioBps: uint16"
    ],
    "receipt": [
      "taskId: bytes32_hex",
      "enqueuedHeight: uint64",
      "kind: enum",
      "accepted: boolean",
      "error?: string"
    ],
    "resultRecord": [
      "taskId: bytes32_hex",
      "completedHeight: uint64",
      "status: enum['Completed','Failed','Expired']",
      "producer: string (provider id / address)",
      "costUnits: uint32",
      "output: object (ai:{text:string}|quantum:{passed:boolean,digest:bytes32_hex})",
      "attestation?: object"
    ]
  },
  "vectors": [
    {
      "name": "ai_basic_ok_next_block",
      "request": {
        "chainId": "animica:devnet",
        "height": 100,
        "txHash": "0xaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa",
