          "device": "IonQ-QPU",
          "shots": 1024,
          "trapsRatioBps": 500,
          "passed": true,
          "digest": "0x8888888888888888888888888888888888888888888888888888888888888888",
          "attestation": {
            "qpu": "ionq-demo",
            "bundle": "0xfeedface"
          }
